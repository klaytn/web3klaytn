#!/usr/bin/env bash
set -e

CURRENT_FILE_DIR=$(cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd)

java -ea                          \
  -Xms512M                        \
  -Xmx1024M                       \
  -server                         \
  -jar "${CURRENT_FILE_DIR}"/libs/web3rpc-openapi-generator-cli.jar "$@"